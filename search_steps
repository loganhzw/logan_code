[1mdiff --git a/cucumber-watir-example/features/support/screenshots.rb b/cucumber-watir-example/features/support/screenshots.rb[m
[1mnew file mode 100644[m
[1mindex 0000000..80728dc[m
[1m--- /dev/null[m
[1m+++ b/cucumber-watir-example/features/support/screenshots.rb[m
[36m@@ -0,0 +1,48 @@[m
[32m+[m[32m# This is an example of how you can set up screenshots for your[m
[32m+[m[32m# browser testing. Just run cucumber with --format html --out report.html[m
[32m+[m[32m#[m
[32m+[m[32m# The code below will work on OS X or Windows (with IE Watir only).[m
[32m+[m[32m# Adding support for other platforms should be easy - as long as there is a[m[41m [m
[32m+[m[32m# ruby library or command line tool to take pictures.[m
[32m+[m[32m#[m
[32m+[m[32m# module Screenshots[m
[32m+[m[32m#   if Cucumber::OS_X[m
[32m+[m[32m#     def embed_screenshot(id)[m
[32m+[m[32m#       `screencapture -t png #{id}.png`[m
[32m+[m[32m#       embed("#{id}.png", "image/png")[m
[32m+[m[32m#     end[m
[32m+[m[32m#   elsif Cucumber::WINDOWS[m
[32m+[m[32m#     # http://wtr.rubyforge.org/rdoc/classes/Watir/ScreenCapture.html[m
[32m+[m[32m#     require 'watir/screen_capture'[m
[32m+[m[32m#     # include 'Watir'[m
[32m+[m[32m#     include Watir::ScreenCapture[m
[32m+[m[32m#     def embed_screenshot(id)[m
[32m+[m[32m#       screen_capture("#{id}.jpg", true)[m
[32m+[m[32m#       embed("#{id}.jpg", "image/jpeg")[m
[32m+[m[32m#     end[m
[32m+[m[32m#   else[m
[32m+[m[32m#     # Other platforms...[m
[32m+[m[32m#     def embed_screenshot(id)[m
[32m+[m[32m#       STDERR.puts "Sorry - no screenshots on your platform yet."[m
[32m+[m[32m#     end[m
[32m+[m[32m#   end[m
[32m+[m[32m# end[m
[32m+[m[32m# World(Screenshots)[m
[32m+[m
[32m+[m[32m# After do[m
[32m+[m[32m#   embed_screenshot("screenshot-#{Time.new.to_i}")[m
[32m+[m[32m# end[m
[32m+[m
[32m+[m[32m# # Other variants:[m
[32m+[m[32m# #[m
[32m+[m[32m# # Only take screenshot on failures[m
[32m+[m[32m# #[m
[32m+[m[32m# #   After do |scenario|[m
[32m+[m[32m# #     embed_screenshot("screenshot-#{Time.new.to_i}") if scenario.failed?[m
[32m+[m[32m# #   end[m
[32m+[m[32m# #[m
[32m+[m[32m# # Only take screenshot for scenarios or features tagged @screenshot[m
[32m+[m[32m# #[m
[32m+[m[32m# #   After(@screenshot) do[m
[32m+[m[32m# #     embed_screenshot("screenshot-#{Time.new.to_i}")[m
[32m+[m[32m# #   end[m
\ No newline at end of file[m
