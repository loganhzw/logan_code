[32m+[m[32mrequire 'watir-webdriver'[m
[32m+[m
[32m+[m[32m#Tutorial code can be found at[m
[32m+[m[32m#http://khughes88.wordpress.com/2010/08/30/cucumber-and-watir-a-quick-tutorial-for-testers/[m
[32m+[m
[32m+[m[32mGiven /^I have gone to the Baidu page$/ do[m
[32m+[m	[32m# @browser = Watir::Browser.new :chrome[m
[32m+[m	[32m@browser.goto("http://www.baidu.com")[m
[32m+[m[32mend[m
[32m+[m
[32m+[m[32mWhen /^I add "(.*?)" to the search box$/ do |item|[m
[32m+[m	[32m@browser.text_field(:id, "kw1").set(item)[m
[32m+[m	[32m#pending # express the regexp above with the code you wish you had[m
[32m+[m[32mend[m
[32m+[m
[32m+[m[32mAnd /^click the Search Button$/ do[m
[32m+[m	[32m@browser.button(:id, "su1").click[m
[32m+[m	[32m#pending # express the regexp above with the code you wish you had[m
[32m+[m[32mend[m
[32m+[m
[32m+[m[32mThen /^"(.*?)" should be mentioned in the results$/ do |arg1|[m
[32m+[m	[32m@browser.div(:id, "content_left").wait_until_present[m
[32m+[m	[32m@browser.div(:id, "content_left").text.should include(arg1)[m
[32m+[m	[32m#@browser.div(:xpath, "//*[@id=\"rso\"]/li[1]/div").hover[m
[32m+[m	[32m#@browser.div(:xpath, "//*[@id=\"rso\"]/li[1]/div/div[2]/div").wait_until_present[m
[32m+[m	[32m# @browser.div(:xpath, "//*[@id=\"rso\"]/li[1]/div/div[2]/div").hover[m
[32m+[m	[32m# @browser.div(:xpath, "//*[@id=\"nycntg\"]").wait_until_present[m
[32m+[m	[32m# @browser.div(:xpath, "//*[@id=\"nycntg\"]").text.downcase.should include("cat")[m
[32m+[m	[32m#pending # express the regexp above with the code you wish you had[m
[32m+[m[32mend[m
\ No newline at end of file[m
